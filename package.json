{
  "name": "typesnitch",
  "version": "0.0.3",
  "description": "A Simple Type Sniffer for JS",
  "main": "src/snitch.js",
  "scripts": {
    "test": "jest --coverage",
    "test:silent": "npm test -- --silent --noStackTrace",
    "watch": "jest --watch --verbose",
    "watchall": "jest --watchAll",
    "pretty": "npx prettier --write src/",
    "build": "npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mfeyx/typesnitch.git"
  },
  "keywords": [
    "types",
    "type sniffer",
    "type detector",
    "type detection",
    "type checking",
    "type snitch",
    "check types",
    "type checker",
    "type check"
  ],
  "author": "Markus Feiks",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mfeyx/typesnitch/issues"
  },
  "homepage": "https://github.com/mfeyx/typesnitch#readme",
  "devDependencies": {
    "husky": "^4.2.5",
    "jest": "^26.1.0",
    "prettier": "2.0.5",
    "pretty-quick": "^2.0.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
